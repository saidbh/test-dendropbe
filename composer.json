{
    "type": "project",
    "license": "proprietary",
    "name": "dendromap/dendromap-api",
    "version": "1.0.2",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.4",
        "creof/doctrine2-spatial": "~1",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^2",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^2",
        "doctrine/orm": "^2",
        "firebase/php-jwt": "^5.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.5",
        "nelmio/api-doc-bundle": "^3.0",
        "nelmio/cors-bundle": "^1.5",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "sendgrid/sendgrid": "~7",
        "sensio/framework-extra-bundle": "^5.2",
        "sensiolabs/security-checker": "4.1.8",
        "spatie/image": "^1.5",
        "stefangabos/zebra_image": "^2.4",
        "stripe/stripe-php": "^6.43",
        "symfony/asset": "4.1.*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/messenger": "*",
        "symfony/property-access": "4.1.*",
        "symfony/property-info": "4.1.*",
        "symfony/requirements-checker": "^2.0",
        "symfony/security-csrf": "*",
        "symfony/serializer": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/yaml": "*",
        "vinyvicente/doctrine-point-type": "^1.1",
        "white-october/pagerfanta-bundle": "^1.3"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.7",
        "symfony/var-dumper": "*",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "vendor/bin/requirements-checker": "php-script",
            "requirements-checker": "script",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.1.*"
        }
    },
    "description": "Dendromap Api build on Symfony 4"
}
